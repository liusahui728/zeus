<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="seqSequence"
		class="org.com.zeus.sequence.handler.SeqGeneratorImpl"
		init-method="init">
		<property name="seqName" value="seq"></property>
		<property name="step" value="10"></property>
	</bean>
	<bean id="seq2Sequence"
		class="org.com.zeus.sequence.handler.SeqGeneratorImpl"
		init-method="init">
		<property name="seqName" value="seq_2"></property>
		<property name="step" value="20"></property>
	</bean>
	<bean id="seq3Sequence"
		class="org.com.zeus.sequence.handler.SeqGeneratorImpl"
		init-method="init">
		<property name="seqName" value="seq_3"></property>
	</bean>
</beans>